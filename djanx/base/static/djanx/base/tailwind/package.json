{
  "name": "djanx-tailwindcss",
  "scripts": {
    "dev:tailwindcss": "npx @tailwindcss/cli -i ./input.css -o ./output.css --watch",
    "dev": "npm run dev:tailwindcss",
    "build:tailwindcss": "npx @tailwindcss/cli -i ./input.css -o ./output.css --minify",
    "build": "npm run build:tailwindcss"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.16",
    "tailwindcss": "^4.1.16"
  }
}
